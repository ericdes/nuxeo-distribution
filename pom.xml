  <parent>
    <groupId>org.nuxeo</groupId>
    <artifactId>nuxeo-ecm</artifactId>
    <version>5.2.0</version>
    <relativePath>../pom.xml</relativePath>
  </parent>

  <groupId>org.nuxeo.ecm.distribution</groupId>
  <artifactId>nuxeo-distribution</artifactId>
  <packaging>pom</packaging>
  <name>Nuxeo Distribution Parent</name>
  <description>Build package distributions of nuxeo applications</description>

  <properties>
    <nuxeo.common.version>1.5.0</nuxeo.common.version>
    <nuxeo.runtime.version>1.5.0</nuxeo.runtime.version>
    <nuxeo.core.version>1.5.0</nuxeo.core.version>
    <nuxeo.theme.version>5.2.0</nuxeo.theme.version>
    <nuxeo.features.version>5.2.0</nuxeo.features.version>
    <nuxeo.webengine.version>5.2.0</nuxeo.webengine.version>
    <nuxeo.services.version>5.2.0</nuxeo.services.version>
    <nuxeo.jsf.version>5.2.0</nuxeo.jsf.version>
    <nuxeo.dm.version>5.2.0</nuxeo.dm.version>
  </properties>

  <profiles>
    <profile>
      <id>nuxeo-ep</id>
      <modules>
        <module>nuxeo-platform-ear</module>
      </modules>
    </profile>
    <profile>
      <id>nuxeo-dm</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <modules>
        <module>nuxeo-platform-ear</module>
        <module>nuxeo-distribution-dm</module>
      </modules>
    </profile>
    <profile>
      <id>nuxeo-ep-jboss</id>
      <modules>
        <module>nuxeo-platform-ear</module>
        <module>nuxeo-distribution-dm</module>
        <module>nuxeo-distribution-jboss</module>
      </modules>
    </profile>
    <profile>
      <id>shell</id>
      <modules>
        <module>nuxeo-distribution-base</module>
        <module>nuxeo-distribution-shell</module>
      </modules>
    </profile>
    <profile>
      <id>jetty</id>
      <modules>
        <module>nuxeo-distribution-base</module>
        <module>nuxeo-distribution-server</module>
        <module>nuxeo-distribution-jetty</module>
      </modules>
    </profile>
    <profile>
      <id>gf3</id>
      <modules>
        <module>nuxeo-distribution-base</module>
        <module>nuxeo-distribution-server</module>
        <module>nuxeo-distribution-gf3</module>
      </modules>
    </profile>
    <profile>
      <id>tomcat</id>
      <modules>
        <module>nuxeo-distribution-base</module>
        <module>nuxeo-distribution-server</module>
        <module>nuxeo-distribution-tomcat</module>
      </modules>
    </profile>
    <profile>
      <id>core</id>
      <modules>
        <module>nuxeo-distribution-base</module>
        <module>nuxeo-distribution-server</module>
      </modules>
    </profile>
    <!-- for deployment purpose -->
    <profile>
      <id>all-distributions</id>
      <modules>
        <module>nuxeo-platform-ear</module>
        <module>nuxeo-distribution-dm</module>
        <module>nuxeo-distribution-jboss</module>
        <module>nuxeo-distribution-base</module>
        <module>nuxeo-distribution-shell</module>
        <module>nuxeo-distribution-server</module>
        <module>nuxeo-distribution-jetty</module>
        <module>nuxeo-distribution-gf3</module>
        <module>nuxeo-distribution-tomcat</module>
      </modules>
    </profile>
  </profiles>

